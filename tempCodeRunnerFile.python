import socket
#CLIENT
HOST = '10.133.7.135'
PORT = 65432

# Crear un socket TCP
with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
    s.connect((HOST, PORT))

    while True:
        message = input("TÃº: ")
        if message.lower() == 'salir':
            break

        s.sendall(message.encode())
        data = s.recv(1024)
        print(f"Servidor: {data.decode()}")